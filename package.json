{
  "name": "interactioner",
  "version": "0.0.1",
  "description": "Makes it easier to add interactions on iNaturalist.",
  "main": "content_script.js",
  "scripts": {
    "dev": "concurrently \"esbuild content_script.ts --bundle --watch --outfile=content_script.js\" \"web-ext run --keep-profile-changes --firefox-profile=test --start-url https://www.inaturalist.org/observations/235761958\"",
    "lint": "web-ext lint --ignore-files tsconfig.json"
  },
  "author": "w-biggs",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.1.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.7.2",
    "vanjs-core": "^1.5.2",
    "web-ext": "^8.3.0"
  }
}
